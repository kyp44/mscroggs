TARGET = answers

.PHONY: all clean

#==============================================================================
all:
	pdflatex $(TARGET).tex
#==============================================================================


#==============================================================================
card:
#==============================================================================
	pdflatex card.tex

#==============================================================================

#==============================================================================
clean:
#==============================================================================
	rm -f *.aux *.log *.pdf *.out *.toc
	rm -f -r *.png figs/
	rm -f -r __pycache__
